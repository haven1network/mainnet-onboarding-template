{
    "compilerOptions": {
        "target": "es2020",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "paths": {
            "@lib": ["./lib/"],
            "@lib/*": ["./lib/*"],
            "@test/*": ["./test/*"],
            "@typechain": ["./typechain-types/"],
            "@typechain/*": ["./typechain-types/*"]
        }
    }
}
